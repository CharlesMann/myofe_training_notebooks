#!/Users/charlesmann/Research/UK/repositories/myofe_training_notebooks/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'FFC==2017.1.0','console_scripts','ffc'
__requires__ = 'FFC==2017.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('FFC==2017.1.0', 'console_scripts', 'ffc')()
    )
